{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "height: fit-content",
          "-webkit-text-size-adjust",
          "background-image: -webkit-image-set()"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}